        }
      ]
    },
    {
      "id": "math_ring1Light",
      "type": "MathClosure",
      "position": {
        "x": 40,
        "y": 1240
      },
      "width": 160,
      "label": "Ring1 Light",
      "params": {
        "source": "// Ring light effect\n// sdf: vec2(sdf, normFactor)\n// uRing1[0]: ring position, uRing1[1]: ring width, uRing1[2]: intensity\nfloat nf = max(sdf.y, 0.00001);\nfloat lit = 1.0 + sdf.x / nf;\nlit = 1.0 - smoothstep(0.0, 1.0, abs((lit - uRing1.x) * uRing1.y));\noutput = lit * uRing1.z;"
      },
      "inputs": [
        {
          "id": "sdf",
          "name": "sdf",
          "type": "vector2"
        },
        {
          "id": "uRing1",
          "name": "uRing1",
          "type": "vector3"
        }
      ],
      "outputs": [
        {
          "id": "output",
          "name": "Output",
          "type": "float"
        }
      ],
      "inputBindings": [
        {
          "portId": "sdf",
