    {
      "id": "math_src1Light",
      "type": "MathClosure",
      "position": {
        "x": 40,
        "y": 640
      },
      "width": 160,
      "label": "Src1 Light",
      "params": {
        "source": "// Gaussian-like light falloff from SDF\n// sdf: vec2(sdf, normFactor)\n// uSrc1[0]: falloff factor, uSrc1[1]: intensity\nfloat nf = max(sdf.y, 0.00001);\nfloat lit = 1.0 + sdf.x / nf;\nlit = exp(-lit * lit * uSrc1.x);\noutput = lit * uSrc1.y;"
      },
      "inputs": [
        {
          "id": "sdf",
          "name": "sdf",
          "type": "vector2"
        },
        {
          "id": "uSrc1",
          "name": "uSrc1",
          "type": "vector2"
        }
      ],
      "outputs": [
        {
          "id": "output",
          "name": "Output",
          "type": "float"
        }
      ],
      "inputBindings": [
        {
          "portId": "sdf",
          "label": "sdf",
          "variableName": "sdf",
          "type": "vector2"
        },
        {
          "portId": "normFactor",
          "label": "normFactor",
          "variableName": "normFactor",
          "type": "any"
        },
        {
          "portId": "uSrc1",
          "label": "uSrc1",
          "variableName": "uSrc1",
          "type": "vector2"
        }
      ]
    },
    {
      "id": "math_src2Light",
      "type": "MathClosure",
      "position": {
        "x": 40,
        "y": 940
      },
      "width": 160,
      "label": "Src2 Light",
      "params": {
        "source": "// Cubic falloff light\n// sdf: vec2(sdf, normFactor)\n// uSrc2[0]: falloff factor, uSrc2[1]: intensity\nfloat nf = max(sdf.y, 0.00001);\nfloat lit = 1.0 + sdf.x / nf;\nlit = 1.0 - max(pow(1.0 - lit * uSrc2.x, 3.0), 0.0);\noutput = lit * uSrc2.y;"
      },
      "inputs": [
        {
          "id": "sdf",
          "name": "sdf",
          "type": "vector2"
        },
        {
          "id": "uSrc2",
          "name": "uSrc2",
          "type": "vector2"
        }
      ],
      "outputs": [
        {
          "id": "output",
          "name": "Output",
          "type": "float"
        }
      ],
      "inputBindings": [
        {
          "portId": "sdf",
