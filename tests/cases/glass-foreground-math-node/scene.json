    {
      "id": "math_boxSdf",
      "type": "MathClosure",
      "position": {
        "x": 320,
        "y": 220
      },
      "width": 160,
      "label": "Box SDF",
      "params": {
        "source": "// Box SDF with rounded corners\n// inputs: p (vec2) - position from center, b (vec2) - half size, r (vec3) - radius in r.z\n// output: signed distance (float)\nfloat radius = r.z;\nvec2 d = (abs(p) - b + radius).xy;\noutput = min(max(d.x, d.y), 0.0) + length(max(d, vec2(0.0))) - radius;"
      },
      "inputs": [
        {
          "id": "p",
          "name": "p",
          "type": "vector4"
        },
        {
          "id": "b",
          "name": "b",
          "type": "vector4"
        },
        {
          "id": "r",
          "name": "r",
          "type": "vector3"
        }
      ],
      "outputs": [
        {
          "id": "output",
          "name": "Output",
          "type": "float"
        }
      ],
      "inputBindings": [
        {
          "portId": "p",
          "label": "p",
          "variableName": "p",
          "type": "vector4"
        },
        {
          "portId": "b",
          "label": "b",
          "variableName": "b",
          "type": "vector4"
        },
        {
          "portId": "r",
          "label": "r",
          "variableName": "r",
          "type": "vector3"
        }
      ]
    },
