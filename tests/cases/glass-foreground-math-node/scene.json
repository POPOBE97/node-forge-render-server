    {
      "id": "math_sdfMorph",
      "type": "MathClosure",
      "position": {
        "x": 540,
        "y": 220
      },
      "width": 160,
      "label": "SDF Shape Morph",
      "params": {
        "source": "// Morph between circle and box SDF\n// uShapeMorph: 0 = circle (pressed), 1 = box (active)\nfloat circleNF = circleNormFactor.w;\nfloat boxNF = boxNormFactor.z;\nfloat sdf = mix(circleSdf, boxSdf, uShapeMorph);\nfloat normFactor = mix(circleNF, boxNF, uShapeMorph);\noutput = vec2(sdf, normFactor);"
      },
      "inputs": [
        {
