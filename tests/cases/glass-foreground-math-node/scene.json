    {
      "id": "math_uvToPixel",
      "type": "MathClosure",
      "position": {
        "x": -400,
        "y": 220
      },
      "width": 160,
      "label": "UV to Pixel",
      "params": {
        "source": "// Convert screen UV to pixel coordinates\nvec2 screenUV = vUv;\nvec2 uvPx = screenUV * uGeoPxSize.xy;\n\n// Calculate center in pixel coords (flip Y)\nvec2 centerPx = uGeoPxSize.xy * 0.5;\ncenterPx.y = uGeoPxSize.y - centerPx.y;\n\noutput = vec4(uvPx, centerPx);"
      },
      "inputs": [
        {
          "id": "vUv",
          "name": "vUv",
          "type": "vector2"
        },
        {
          "id": "uGeoPxSize",
          "name": "uGeoPxSize",
          "type": "vector3"
        }
      ],
      "outputs": [
        {
          "id": "output",
          "name": "Output",
          "type": "vector4"
        }
      ],
      "inputBindings": [
        {
          "portId": "vUv",
          "label": "vUv",
          "variableName": "vUv",
          "type": "vector2"
        },
        {
          "portId": "uGeoPxSize",
          "label": "uGeoPxSize",
          "variableName": "uGeoPxSize",
          "type": "vector3"
        }
      ]
    },
    {
      "id": "math_calcHalfSize",
      "type": "MathClosure",
      "position": {
        "x": -160,
        "y": -60
      },
      "width": 160,
      "label": "Calculate Half Size",
      "params": {
        "source": "// Calculate half size and normalization factors\nvec2 halfSizePx = uGeoPxSize.xy * 0.5;\nfloat boxNormFactor = min(halfSizePx.x, halfSizePx.y);\nfloat circleRadius = boxNormFactor; // min of half width/height\noutput = vec4(halfSizePx, boxNormFactor, circleRadius);"
      },
      "inputs": [
        {
          "id": "uGeoPxSize",
          "name": "uGeoPxSize",
          "type": "vector3"
        }
      ],
      "outputs": [
        {
          "id": "output",
          "name": "Output",
          "type": "vector4"
        }
      ],
      "inputBindings": [
        {
          "portId": "uGeoPxSize",
          "label": "uGeoPxSize",
          "variableName": "uGeoPxSize",
          "type": "vector3"
        }
      ]
    },
    {
      "id": "math_calcLightCenter",
      "type": "MathClosure",
      "position": {
        "x": -400,
        "y": 620
      },
      "width": 160,
      "label": "Calculate Light Center",
      "params": {
        "source": "// Calculate light center position in pixel coords\n// uLightCenter is normalized coords (0~1), need to flip Y\nvec2 lightCenterPx = vec2(uLightCenter.x, 1.0 - uLightCenter.y) * uGeoPxSize.xy;\noutput = lightCenterPx;"
      },
      "inputs": [
        {
          "id": "uLightCenter",
          "name": "uLightCenter",
          "type": "vector2"
        },
