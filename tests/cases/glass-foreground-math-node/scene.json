    {
      "id": "math_artisticMapping",
      "type": "MathClosure",
      "position": {
        "x": 540,
        "y": 800
      },
      "width": 160,
      "label": "Artistic Mapping",
      "params": {
        "source": "// Artistic alpha mapping with squared falloff and edge limit\n// sdf: vec2(sdf, normFactor)\nfloat finalAlpha = combinedLight * combinedLight;\nfinalAlpha = clamp(finalAlpha, 0.0, 1.0);\n\n// Edge limit based on SDF\nfloat nf = max(sdf.y, 0.00001);\nfloat limit = 1.0 + sdf.x / nf;\nfinalAlpha *= max(1.0 - pow(limit, 8.0), 0.0);\n\n// Apply strength\nfinalAlpha *= uStrength;\n\noutput = finalAlpha;"
      },
      "inputs": [
        {
          "id": "combinedLight",
          "name": "combinedLight",
          "type": "float"
        },
        {
          "id": "sdf",
          "name": "sdf",
          "type": "vector2"
        },
        {
          "id": "uStrength",
          "name": "uStrength",
          "type": "float"
        }
      ],
      "outputs": [
        {
          "id": "output",
          "name": "Output",
          "type": "float"
        }
      ],
      "inputBindings": [
        {
          "portId": "combinedLight",
          "label": "combinedLight",
          "variableName": "combinedLight",
          "type": "float"
        },
        {
          "portId": "sdf",
          "label": "sdf",
          "variableName": "sdf",
          "type": "vector2"
        },
        {
          "portId": "normFactor",
          "label": "normFactor",
          "variableName": "normFactor",
          "type": "any"
        },
        {
          "portId": "uStrength",
          "label": "uStrength",
          "variableName": "uStrength",
          "type": "float"
        }
      ]
    },
