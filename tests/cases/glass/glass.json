    {
      "id": "math_finalColor",
      "type": "MathClosure",
      "position": {
        "x": 820,
        "y": 480
      },
      "width": 240,
      "label": "Final Glass Color",
      "params": {
        "source": "// Combine all glass effects into final color\n// Apply HSV lighting boost\nvec3 hsv;\n{\n  vec4 K = vec4(0.0, -1.0 / 3.0, 2.0 / 3.0, -1.0);\n  vec4 p = mix(vec4(uGlassColor.bg, K.wz), vec4(uGlassColor.gb, K.xy), step(uGlassColor.b, uGlassColor.g));\n  vec4 q = mix(vec4(p.xyw, uGlassColor.r), vec4(uGlassColor.r, p.yzx), step(p.x, uGlassColor.r));\n  float d = q.x - min(q.w, q.y);\n  float e = 1e-10;\n  hsv = vec3(abs(q.z + (q.w - q.y) / (6.0 * d + e)), d / (q.x + e), q.x);\n}\n\n// Add lighting to value\nhsv.z += lighting * 1.2 * (1.0 - luma * 0.5);\nhsv.z = clamp(hsv.z, 0.0, 1.0);\n\n// Convert back to RGB\nvec3 rgb;\n{\n  vec4 K = vec4(1.0, 2.0 / 3.0, 1.0 / 3.0, 3.0);\n  vec3 p = abs(fract(hsv.xxx + K.xyz) * 6.0 - K.www);\n  rgb = hsv.z * mix(K.xxx, clamp(p - K.xxx, 0.0, 1.0), hsv.y);\n}\n\n// Apply saturation and brightness adjustments\nfloat luminance = dot(rgb, vec3(0.2125, 0.7153, 0.0721));\nvec3 adjustedSat = uSaturation * rgb + (1.0 - uSaturation) * vec3(luminance);\nvec3 adjustedBright = adjustedSat + vec3(uBrightness);\n\n// Calculate final alpha\nfloat finalAlpha = smoothstep(0.0, 10.0, -shapeSdf);\nfinalAlpha *= uAlpha * uStrength;\n\nvec4 outColor = vec4(adjustedBright, finalAlpha);\noutColor.rgb *= outColor.a; // Premultiply alpha\n\noutput = outColor;"
      },
