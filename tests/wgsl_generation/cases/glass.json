    {
      "id": "math_shapeSdf",
      "type": "MathClosure",
      "position": {
        "x": 560,
        "y": -320
      },
      "width": 220,
      "label": "Shape SDF with Edge",
      "params": {
        "source": "// Apply edge curve to SDF\n// curve(x, powRatio) implementation\nfloat d = baseSdf;\nfloat edge = uShapeEdgePx;\n\nif (d < -edge) {\n  d = -edge;\n} else if (d < 0.0) {\n  float per = -d / edge;\n  // smooth7_vertical approximation\n  float t = pow(clamp(per, 0.0, 1.0), 0.5);\n  t = mix(0.5, 1.0, t);\n  t = clamp(t, 0.0, 1.0);\n  float t2=t*t; \n  float t3=t2*t;\n  float t4=t3*t;\n  float t5=t4*t;\n  float t6=t5*t;\n  float t7=t6*t;\n  t = -20.0*t7 + 70.0*t6 - 84.0*t5 + 35.0*t4;\n  t = (t - 0.5) * 2.0;\n  // Apply power curve\n  float circlePow = 1.0 - pow(1.0 - t, uShapeEdgePow);\n  d = -circlePow * edge;\n}\n\noutput = d;"
      },
      "inputs": [
        {
          "id": "baseSdf",
          "name": "baseSdf",
          "type": "float"
        },
        {
          "id": "uShapeEdgePx",
          "name": "uShapeEdgePx",
          "type": "float"
        },
        {
          "id": "uShapeEdgePow",
          "name": "uShapeEdgePow",
          "type": "float"
        }
      ],
      "outputs": [
        {
          "id": "output",
          "name": "Output",
          "type": "float"
        }
      ],
      "inputBindings": [
        {
          "portId": "baseSdf",
          "label": "baseSdf",
          "variableName": "baseSdf",
          "type": "float",
          "sourceBinding": {
            "nodeId": "math_boxSdf",
            "outputPortId": "output",
            "outputLabel": "Output"
          }
        },
        {
          "portId": "uShapeEdgePx",
          "label": "uShapeEdgePx",
          "variableName": "uShapeEdgePx",
          "type": "float",
          "sourceBinding": {
            "nodeId": "uniform_uShapeEdgePx",
            "outputPortId": "value",
            "outputLabel": "Value"
          }
        },
        {
