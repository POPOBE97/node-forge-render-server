    {
      "id": "math_calculateLighting",
      "type": "MathClosure",
      "position": {
        "x": 560,
        "y": 1260
      },
      "width": 220,
      "label": "Calculate Lighting",
      "params": {
        "source": "// Directional lighting calculation\nvec3 normalizedLightDir = normalize(uDirectionalLightDir);\nfloat dotProduct = dot(normal, normalizedLightDir);\n\n// Reflection angle factor\nfloat reflectionAngle = acos(clamp(dotProduct, -1.0, 1.0));\nfloat angleFactor = 1.0 - (reflectionAngle / (3.14159 * 0.5));\n\nfloat adjustedIntensity = uDirectionalLightIntensity * angleFactor;\nadjustedIntensity = max(adjustedIntensity, 0.0);\n\nfloat lighting = max(dotProduct, 0.0) * adjustedIntensity;\n\noutput = lighting;"
      },
      "inputs": [
        {
          "id": "normal",
          "name": "normal",
          "type": "vector3"
        },
        {
          "id": "uDirectionalLightDir",
