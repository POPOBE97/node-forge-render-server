    {
      "id": "math_boxSdf",
      "type": "MathClosure",
      "position": {
        "x": 240,
        "y": 100
      },
      "width": 220,
      "label": "Box SDF",
      "params": {
        "source": "// Rounded box SDF\n// box(p, b, r) = min(max(d.x, d.y), 0.0) + length(max(d, 0.0)) - r\nvec2 p = uvPx - centerPx;\nvec2 b = uGeoPxSize.xy * 0.5;\nfloat r = uGeoPxSize.z;\n\nvec2 d = abs(p) - b + r;\nfloat sdf = min(max(d.x, d.y), 0.0) + length(max(d, 0.0)) - r;\n\noutput = sdf;"
      },
      "inputs": [
        {
          "id": "uvPx",
          "name": "uvPx",
          "type": "vector2"
        },
        {
          "id": "centerPx",
          "name": "centerPx",
          "type": "vector2"
        },
